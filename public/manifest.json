{
  "manifest_version": 3,
  "name": "kurukuruz demo extension",
  "icons": {
    "48":"icon.png"
  },
  "version": "0.1.1",
  "description": "ページのハイパーリンクを取得してクリップボードにコピーする拡張機能",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "このページへのハイパーリンクをコピー",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}